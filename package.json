{
  "name": "ttjs",
  "version": "1.0.0",
  "description": "Tongue Tied as JS",
  "main": "index.js",
  "engines": {
    "node": "0.12.x"
  },
  "scripts": {
    "start": "NODE_ENV=dev node src/server/index",
    "postinstall": "webpack",
    "test": "MONGO_CONNECTION_STRING=mongodb://localhost/test npm run karma && npm run mocha",
    "karma": "MONGO_CONNECTION_STRING=mongodb://localhost/test karma start --single-run",
    "mocha": "MONGO_CONNECTION_STRING=mongodb://localhost/test mocha src/server/__tests__/*-test.js",
    "mocha-watch": "MONGO_CONNECTION_STRING=mongodb://localhost/test mocha src/server/__tests__/*-test.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/funkfinger/ttjs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/funkfinger/ttjs/issues"
  },
  "homepage": "https://github.com/funkfinger/ttjs",
  "dependencies": {
    "bluebird": "^2.9.30",
    "body-parser": "^1.13.1",
    "css-loader": "^0.15.2",
    "express": "^4.12.4",
    "file-loader": "^0.8.4",
    "modernizr": "^3.0.0-alpha.4",
    "mongodb": "^2.0.34",
    "mongoose": "^4.0.5",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.7",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^0.15.0",
    "gulp-mocha": "^2.1.1",
    "jasmine": "^2.3.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.36",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-jasmine": "^0.3.5",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.17",
    "sinon": "^1.15.3",
    "supertest": "^1.0.1",
    "supertest-as-promised": "^2.0.1",
    "webpack-dev-server": "^1.10.1"
  }
